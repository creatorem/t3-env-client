{
  "name": "t3-env-client",
  "version": "1.0.4",
  "engines": {
    "node": ">=v21"
  },
  "author": {
    "url": "https://creatorem.com",
    "name": "Arnaud Pfundstein"
  },
  "license": "MIT",
  "type": "module",
  "bin": {
    "t3-env-client": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "pnpm build:nextjs && pnpm build:cli",
    "build:cli": "tsup",
    "build:nextjs": "next build",
    "dev": "pnpm build:cli && cd ../example && node ../core/dist/cli/index.js --verbose",
    "dev:help": "cd ../example && node ../core/dist/cli/index.js --help",
    "clean": "git clean -xdf node_modules dist .next",
    "format": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "lint": "turbo lint --cache-dir=.turbo --affected --continue -- --cache --cache-location 'node_modules/.cache/.eslintcache' && manypkg check",
    "lint:fix": "turbo lint --cache-dir=.turbo --affected --continue -- --fix --cache --cache-location 'node_modules/.cache/.eslintcache' && manypkg fix",
    "typecheck": "turbo typecheck --affected --cache-dir=.turbo",
    "test": "turbo test --cache-dir=.turbo"
  },
  "packageManager": "pnpm@9.12.0",
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@t3-oss/env-nextjs": "0.13.8",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.2",
    "esbuild": "^0.27.0",
    "log-symbols": "^7.0.1",
    "lucide-react": "^0.544.0",
    "mime-types": "^3.0.2",
    "next": "16.0.3",
    "ora": "^9.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/postcss": "^4.1.17",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "consola": "^3.4.2",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^4.1.13",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}